    419          static void WSN_TestApp1_HandleKeys( uint8 shift, uint8 keys )
    420          {
    421            zAddrType_t dstAddr;
    422          
    423            // Shift is used to make each button/switch dual purpose.
    424            if ( shift )
    425            {
    426              if ( keys & HAL_KEY_SW_1 )
    427              {
    428              }
    429              if ( keys & HAL_KEY_SW_2 )
    430              {
    431              }
    432              if ( keys & HAL_KEY_SW_3 )
    433              {
    434              }
    435              if ( keys & HAL_KEY_SW_4 )
    436              {
    437              }
    438            }
    439            else
    440            {
    441              if ( keys & HAL_KEY_SW_1 )
    442              {
    443                // Since SW1 isn't used for anything else in this application...
    444          #if defined( SWITCH1_BIND )
    445                // we can use SW1 to simulate SW2 for devices that only have one switch,
    446                keys |= HAL_KEY_SW_2;
    447          #elif defined( SWITCH1_MATCH )
    448                // or use SW1 to simulate SW4 for devices that only have one switch
    449                keys |= HAL_KEY_SW_4;
    450          #endif
    451              }
    452          
    453              if ( keys & HAL_KEY_SW_2 )
    454              {
    455                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
